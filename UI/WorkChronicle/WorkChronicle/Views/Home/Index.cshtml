<select id="dropdown" class="form-select" aria-label="Default select example"></select>

<script>
    let ddl = new Dropdown("#dropdown", "text", "value");

    let data = [
        { text: "hello1", value: 1 },
        { text: "hello2", value: 2 },
        { text: "hello3", value: 3 }
    ];

    ddl.SetData(data);
</script>