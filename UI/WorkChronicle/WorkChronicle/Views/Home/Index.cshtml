@{
    ViewData["Title"] = "Home Page";
}
@model WorkChronicle.ViewModels.ProjectListViewModel

<div class="row">
    <div class="col-6">
        <h3>Projects:</h3>
        <a class="btn btn-sm btn-primary" asp-action="CreateProject">Add new Project</a>
        @foreach (var project in Model.Projects)
        {
            <div class="block">
                @project.Name
            </div>
        }
    </div>
</div>